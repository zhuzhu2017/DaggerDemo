<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:padding="10dp"
    tools:context="com.tongtong.daggerdemo.MainActivity">

    <Button
        android:id="@+id/btn_inject"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Inject基础用法" />

    <Button
        android:id="@+id/btn_inject_relavant"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:text="含引用关系的Inject用法" />

    <Button
        android:id="@+id/btn_inject_custom"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:text="Named和Qulifier的使用" />

</LinearLayout>
